{
    "name": "skore-labs/laravel-auditable",
    "description": "Audit the users that performs actions to your application models",
    "license": "MIT",
    "authors": [
        {
            "name": "RubeÃÅn Robles",
            "email": "me@d8vjork.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/database": "^9.0 || ^10.0 || ^11.0",
        "illuminate/support": "^9.0 || ^10.0 || ^11.0"
    },
    "require-dev": {
        "larastan/larastan": "^2.0",
        "orchestra/testbench": "^7.0 || ^8.0 || ^9.0",
        "phpstan/phpstan": "^1.0",
        "phpunit/phpunit": "^9.0 || ^10.0"
    },
    "autoload": {
        "psr-4": {
            "SkoreLabs\\LaravelAuditable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SkoreLabs\\LaravelAuditable\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "SkoreLabs\\LaravelAuditable\\ServiceProvider"
            ]
        }
    }
}