{
  "name": "skore-labs/laravel-auditable",
  "description": "Simple audit for users that performs actions to your Eloquent models",
  "license": "MIT",
  "authors": [
    {
      "name": "RubeÃÅn Robles",
      "email": "me@d8vjork.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/database": "^11.0 || ^12.0",
    "illuminate/support": "^11.0 || ^12.0"
  },
  "require-dev": {
    "larastan/larastan": "^3.0",
    "orchestra/testbench": "^9.0 || ^10.0",
    "phpstan/phpstan": "^2.0",
    "phpunit/phpunit": "^11.0"
  },
  "autoload": {
    "psr-4": {
      "SkoreLabs\\LaravelAuditable\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SkoreLabs\\LaravelAuditable\\Tests\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "SkoreLabs\\LaravelAuditable\\ServiceProvider"
      ]
    }
  }
}
