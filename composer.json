{
    "name": "skore-labs/laravel-auditable",
    "description": "Audit the users that performs actions to your application models",
    "license": "MIT",
    "authors": [
        {
            "name": "RubeÃÅn Robles",
            "email": "me@d8vjork.com"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2",
        "illuminate/database": "^9.0|^10.0",
        "illuminate/support": "^9.0|^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0",
        "nunomaduro/larastan": "^2.0",
        "phpstan/phpstan": "^1.0",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "SkoreLabs\\LaravelAuditable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SkoreLabs\\LaravelAuditable\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "SkoreLabs\\LaravelAuditable\\ServiceProvider"
            ]
        }
    }
}